<script type="text/javascript">
    mixpanel.track("Dashboard");
    $(document).ready(function() {
        var password_check = '1';
        if (password_check == false) $("#show_password").trigger("click");
        $("#simpan_password").click(function() {
            if ($("#new-password").val() != $("#re-new-password").val()) $("#error_password").show();
            else {
                $("#error_password").hide();
                $("#password_add").submit();
            }
        });

        $("#btn-password").on("click", function() {
            var x = document.getElementById("new-password");
            if (x.type === "password") {
                x.type = "text";
                $("#btn-password")
                    .find("img")
                    .attr("src", "themes/user-v2/img/icon/ic_eye.svg");
            } else {
                x.type = "password";
                $("#btn-password")
                    .find("img")
                    .attr("src", "themes/user-v2/img/icon/ic_eye_slash.svg");
            }
        });

        $("#btn-repassword").on("click", function() {
            var x = document.getElementById("rere-new-password");
            if (x.type === "password") {
                x.type = "text";
                $("#btn-repassword")
                    .find("img")
                    .attr("src", "themes/user-v2/img/icon/ic_eye.svg");
            } else {
                x.type = "password";
                $("#btn-repassword")
                    .find("img")
                    .attr("src", "themes/user-v2/img/icon/ic_eye_slash.svg");
            }
        });
    });

    // Load Overview
    $(document).ready(function() {
        $.ajax({
            url: 'https://aplikasi.finansialku.com/overview',
            success: function(data) {
                $('.overview_finansialku').removeClass('v-middle-content');
                $('.overview_finansialku').html(data).hide().fadeIn('fast');
            }
        })
    });

    function image_modal(type, id) {
        $('input[name=image_image]').val('');

        $('input[name=image_type').val(type);
        $('input[name=image_id').val(id);

        $('#modal_image').modal();
    }

    // setTimeout(news(), 350000);
</script>
<script>
$(document).ready(function(){

    // your code goes here
    $('.alert_delete').each(function(){
        $(this).on("click", function(){
            $this = $(this);
            var itemId = $this.attr('data-id');
            var itemType = $this.attr('data-type');
            var urlAction = 'https://aplikasi.finansialku.com/welcome/financial_plan_delete/'+itemType+'/'+itemId;
            $('.sa-confirm-button-container').find('.confirm').removeAttr('disabled');
            swal({
                title: "Apakah anda yakin?",
                text: "Data tidak akan bisa di kembalikan lagi!",
                showCancelButton: true,
                confirmButtonColor: "#EF5350",
                confirmButtonText: "Ya, Hapus data!",
                cancelButtonText: "Tidak, Batalkan!",
                closeOnConfirm: false,
                closeOnCancel: false,
                imageUrl: 'themes/user-v2/img/alert_warning.png',        
                imageHeight: 100, 
                imageWidth: 100,
            },
            function(isConfirm){
                if (isConfirm) {
                    $('.sa-button-container').find('.cancel').hide();
                    $('.sa-confirm-button-container').find('.confirm').attr('disabled');
                    $('.sa-confirm-button-container').find('.confirm').html('<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>Loading...');
                    $.ajax({
                        type: 'GET',
                        url: urlAction,
                        dataType: "text",
                        success: function(data) {
                            swal({
                                title: "Data di hapus!",
                                text: "Data anda telah dihapus.",
                                confirmButtonColor: "#66BB6A",
                                type: "success"
                            });
                            //$(".msg_area").html(data);
                            //disload(".panel-loading","unblock");
                            function wait(){
                                location.reload();
                            }
                            setTimeout(wait, 500);
                        },
                        error: function() {
                        }
                    });
                } else {
                    swal({
                        title: "Dibatalkan",
                        text: "File anda tidak di hapus :)",
                        confirmButtonColor: "#2196F3",
                        type: "error"
                    });
                }
            });
        });
    });
});
</script>                                            <!-- <div class="footer text-muted">
                            &copy; 2020. <a href="#">Finansialku</a>
                        </div> -->
